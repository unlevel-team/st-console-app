{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "./src/stConsoleApp/" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "opts": {
        "template": "templates/default",  // same as -t templates/default
        "encoding": "utf8",               // same as -e utf8
        "destination": "./jsdoc_out/stConsoleApp/",          // same as -d ./out/
        "recurse": true            // same as -r
        // "tutorials": "./jsdoc_out/_tutorials" // same as -u path/to/tutorials
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}